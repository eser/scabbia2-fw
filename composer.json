{
    "name": "scabbiafw/scabbia2-fw",
    "description": "Scabbia2 PHP Framework Code",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "larukedi",
        "scabbia",
        "scabbia2",
        "scabbiafw",
        "framework"
    ],
    "homepage": "http://www.scabbiafw.com/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Scabbia Framework Organization",
            "email": "contact@scabbiafw.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "psr/log": "1.0.0",
        "scabbiafw/scabbia2-helpers": "*",
        "scabbiafw/scabbia2-interfaces": "*",
        "scabbiafw/scabbia2-lightstack": "*",
        "scabbiafw/scabbia2-router": "*",
        "scabbiafw/scabbia2-tasks": "*",
        "scabbiafw/scabbia2-testing": "*",
        "scabbiafw/scabbia2-yaml": "*"
    },
    "suggest": {
        "ext-curl": "ext/curl for http communication",
        "ext-mbstring": "ext/mbstring for multibyte string manipulation",
        "ext-tokenizer": "ext/tokenizer for lexical analysis"
    },
    "autoload": {
        "psr-4": {
            "Scabbia\\": [ "src/" ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scabbia\\Tests\\": [ "tests/" ]
        }
    }
}
