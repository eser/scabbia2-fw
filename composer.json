{
    "name": "scabbiafw/scabbia2-fw",
    "description": "Scabbia2 PHP Framework Code",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    },
    "keywords": [
        "larukedi",
        "scabbia",
        "scabbia2",
        "scabbiafw",
        "framework"
    ],
    "homepage": "http://www.scabbiafw.com/",
    "license": [
        "Apache-2.0"
    ],
    "authors": [
        {
            "name": "Scabbia Framework Organization",
            "homepage": "http://scabbiafw.com/"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/scabbiafw/scabbia2-fw/blob/master/contributors.md"
        }
    ],
    "support": {
        "issues": "https://github.com/scabbiafw/scabbia2-fw/issues",
        "source": "https://github.com/scabbiafw/scabbia2-fw"
    },
    "require": {
        "php": ">=5.6.0",
        "scabbiafw/scabbia2-config": "*",
        "scabbiafw/scabbia2-events": "*",
        "scabbiafw/scabbia2-formatters": "*",
        "scabbiafw/scabbia2-helpers": "*",
        "scabbiafw/scabbia2-lightstack": "*",
        "scabbiafw/scabbia2-router": "*",
        "scabbiafw/scabbia2-scanners": "*",
        "scabbiafw/scabbia2-services": "*",
        "scabbiafw/scabbia2-tasks": "*",
        "scabbiafw/scabbia2-yaml": "*"
    },
    "require-dev": {
        "scabbiafw/scabbia2-testing": "*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "suggest": {
        "ext-curl": "ext/curl for http communication",
        "ext-mbstring": "ext/mbstring for multibyte string manipulation",
        "ext-tokenizer": "ext/tokenizer for lexical analysis"
    },
    "autoload": {
        "psr-4": {
            "Scabbia\\": [ "src/" ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scabbia\\Tests\\": [ "tests/" ]
        }
    }
}
